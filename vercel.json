{
  "version": 2,
  "name": "fastfood-frontend",
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "API_URL": "https://fastfood-api.onrender.com"
  },
  "regions": ["gru1"],
  "public": true
} 